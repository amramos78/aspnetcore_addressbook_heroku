@model IEnumerable<Contact>
@{
    ViewData["Title"] = "Contact View";
}
<a asp-action="Create" class="btn btn-primary pull-right">Create New Contact</a>
<h1>Contacts</h1>
<hr>
<div class="row">
    <div class="col-md-12">
    @foreach(var c in Model)
    {
        @Html.Partial("ContactPartialView", c)
    }
    </div>
</div>
